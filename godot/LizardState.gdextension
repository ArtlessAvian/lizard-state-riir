[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = false

[libraries]
linux.debug.x86_64 =     "res://../target/debug/libgodot_glue.so"
linux.release.x86_64 =   "res://../target/release/libgodot_glue.so"
windows.debug.x86_64 =   "res://../target/debug/godot_glue.dll"
windows.release.x86_64 = "res://../target/release/godot_glue.dll"
macos.debug =            "res://../target/debug/libgodot_glue.dylib"
macos.release =          "res://../target/release/libgodot_glue.dylib"
macos.debug.arm64 =      "res://../target/debug/libgodot_glue.dylib"
macos.release.arm64 =    "res://../target/release/libgodot_glue.dylib"